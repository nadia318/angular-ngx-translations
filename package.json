{
  "name": "angular-translations",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run angular-translations:serve-ssr",
    "build:dev": "ng build -c=dev && ng run angular-translations:server:dev",
    "build:prod": "ng build --prod && ng run angular-translations:server:production",
    "serve:dev": "PORT=7000 node dist/server/main.js",
    "serve:prod": "NODE_ENV=production && PORT=7700 node dist/server/main.js",
    "prerender": "ng run angular-translations:prerender",
    "i18n-merge": "xliffmerge en-US fr-FR"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/localize": "^10.1.5",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/platform-server": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@gilsdav/ngx-translate-router": "^3.1.3",
    "@gilsdav/ngx-translate-router-http-loader": "^1.1.2",
    "@nguniversal/express-engine": "^10.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "express": "^4.15.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "engines": {
    "node": "^10.13.0 || ^12.16.1",
    "npm": "^6.11.0"
  }
}
